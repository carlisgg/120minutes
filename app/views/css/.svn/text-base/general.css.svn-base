}

.span-center {
	margin: 0 auto;
	float: none;
}

.doa-important-text {
	font-size: 16px;
	font-weight: bold;
}

.doa-red-heading {
	background-image: url("../img/aux/red-triangle.png");
	background-repeat: no-repeat;
	background-position: center left;
	padding-left: 35px;
}

.doa-green-heading {
	background-image: url("../img/aux/green-triangle.png");
	background-repeat: no-repeat;
	background-position: center left;
	padding-left: 35px;
}

.doa-grey-background {
	background-color: #eeefef;
}

.doa-texture-1 {	
	background-image: url("../img/texture-eeefef1x.png");
	background-repeat: repeat;
}

.doa-texture-1 .embossed {
	text-shadow: -1px 1px 0 white;
	font-weight: 500;
}

.doa-texture-2 {
	background-image: url("../img/texture-4546461x.png");
	background-repeat: repeat;
	min-height: 40px;
	color: white;
}

.doa-texture-2 a {
	color: white;
}

.doa-texture-3 {
	background-color: #CE0025;
	color: white;
	
	-webkit-background-size: 10px 10px;
	-moz-background-size: 10px 10px;
	background-size: 10px 10px;
	
	background-image: -webkit-gradient(linear, 0 0, 100% 100%,
					  color-stop(.25, rgba(69, 70, 70, .2)), color-stop(.25, transparent),
					  color-stop(.5, transparent), color-stop(.5, rgba(69, 70, 70, .2)),
					  color-stop(.75, rgba(69, 70, 70, .2)), color-stop(.75, transparent),
					  to(transparent));
	background-image: -webkit-linear-gradient(-45deg, rgba(69, 70, 70, .2) 25%, transparent 25%,
					  transparent 50%, rgba(69, 70, 70, .2) 50%, rgba(69, 70, 70, .2) 75%,
					  transparent 75%, transparent);
	background-image: -moz-linear-gradient(-45deg, rgba(69, 70, 70, .2) 25%, transparent 25%,
					  transparent 50%, rgba(69, 70, 70, .2) 50%, rgba(69, 70, 70, .2) 75%,
					  transparent 75%, transparent);
	background-image: -ms-linear-gradient(-45deg, rgba(69, 70, 70, .2) 25%, transparent 25%,
					  transparent 50%, rgba(69, 70, 70, .2) 50%, rgba(69, 70, 70, .2) 75%,
					  transparent 75%, transparent);
	background-image: -o-linear-gradient(-45deg, rgba(69, 70, 70, .2) 25%, transparent 25%,
					  transparent 50%, rgba(69, 70, 70, .2) 50%, rgba(69, 70, 70, .2) 75%,
					  transparent 75%, transparent);
	background-image: linear-gradient(-45deg, rgba(69, 70, 70, .2) 25%, transparent 25%,
					  transparent 50%, rgba(69, 70, 70, .2) 50%, rgba(69, 70, 70, .2) 75%,
					  transparent 75%, transparent);
}

