/* Solucion responsive para video central de home */

.flex-video {
	position: relative;
	padding-top: 25px;
	padding-bottom: 40%;
	height: 0;
	margin-bottom: 16px;
	margin-top: 16px;
	overflow: hidden;
}

.flex-video.widescreen { 
	padding-bottom: 57.25%; 
}
.flex-video.vimeo { 
	padding-top: 0; 
}

.flex-video iframe,
.flex-video object,
.flex-video embed {
	position: absolute;
	top: 0;
	left: 0;
	width: 100%;
	height: 100%;
}

@media only screen and (max-device-width: 800px), only screen and (device-width: 1024px) and (device-height: 600px), only screen and (width: 1280px) and (orientation: landscape), only screen and (device-width: 800px), only screen and (max-width: 767px) {
  .flex-video { padding-top: 0; }
}

/* Fin de solucion responsive para video central de la home */
